<% if @success -%>
	jQuery('#<%=@app_feed.dom_id%>').replaceWith(decodeHtml('<%= escape_javascript(render('app_feeds/app_feed', :app_item => @app_feed)) %>'));
	jQuery.fancybox.close();
	apply_content_fancybox();
<% else -%>
	<%= output_errors_ajax('messages-container', 'Please correct the following errors:', {:class => 'help-box'}, @app_feed, true) %>
<% end -%>