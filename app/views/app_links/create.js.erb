<% if @success -%>
	jQuery('#<%=@item_id%>').replaceWith('<%= escape_javascript(render('app_links/app_link', :app_item => @app_link)) %>');
	apply_content_fancybox();
<% else -%>
	<%= output_errors_ajax('messages-container', 'Please correct the following errors:', {:class => 'help-box'}, @app_link, true) %>
<% end -%>
