<title><%= @page_title || GlobalConfig.application_name %></title>
<meta http-equiv="content-type" content="text/xhtml; charset=utf-8" />
<meta http-equiv="imagetoolbar" content="no" />
<meta name="distribution" content="all" />
<meta name="robots" content="all" />	
<meta name="resource-type" content="document" />
<meta name="MSSmartTagsPreventParsing" content="true" />
<%= stylesheet_link_tag 'blueprint/print.css', :media => "print" %>
<!--[if IE]><link rel="stylesheet" href="/stylesheets/blueprint/ie.css" type="text/css" media="screen, projection"><![endif]-->
<%= google_load_jquery_ui_css(http_protocol, 'smoothness', '1.8.1') %>
<%= stylesheet_link_tag %W{ jquery/jquery.fancybox SprySlidingPanels formtastic.css default }, :cache => true %>
<%#= google_load_jquery(http_protocol, '1.4.2') %>
<%#= google_load_jquery_ui(http_protocol, '1.8.1') %>
<%= javascript_include_tag %w{
  jquery/jquery.js
  jquery/jquery-ui.js
  jquery/jquery.form.js
  jquery/jquery.tips.js
  jquery/jquery.timers.js
  jquery/jquery.easing.js
  jquery/jquery.fancybox.js
  SprySlidingPanels.js
  application.js }, :cache => 'all_js_cached' %>
<%= javascript_tag %[const AUTH_TOKEN = #{form_authenticity_token.inspect};] if protect_against_forgery? %>
<%= yield :head -%>